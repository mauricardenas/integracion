package com.grupogtd.integration.sicret.jsd.ws.exception;

/**
 * Created by jespinosa on 13/06/2017.
 */
public class ErrorNotificationException extends InteSicretJsdWSBusinessException {

    public String getCode(){
        return ErrorCode.NOTIFICATION_ERROR;
    }

    public ErrorNotificationException()
    {
        super(ErrorMessage.MESAAGE_ERROR_ENVIAR_CORREO);
    }

    public ErrorNotificationException(String message)
    {
        super(message);
    }

    public ErrorNotificationException(String message, Throwable cause)
    {
        super(message, cause);
    }

    public ErrorNotificationException(Throwable cause)
    {
        super(cause);
    }
}
