package com.grupogtd.integration.sicret.jsd.ws.exception;

/**
 * Base Business Excepion
 */
public class InteSicretJsdWSBusinessException extends Exception
{
    public String getCode(){
        return ErrorCode.BASE;
    }

    public InteSicretJsdWSBusinessException()
    {
        super(ErrorMessage.BASE);
    }

    public InteSicretJsdWSBusinessException(String message)
    {
        super(message);
    }

    public InteSicretJsdWSBusinessException(String message, Throwable cause)
    {
        super(message, cause);
    }

    public InteSicretJsdWSBusinessException(Throwable cause)
    {
        super(cause);
    }

}
